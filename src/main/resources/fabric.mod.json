{
  "schemaVersion": 1,
  "id": "dimteleport",
  "version": "${version}",

  "name": "Dimensional Teleport",
  "description": "Adds <dimension> argument to the `/teleport` command.",
  "authors": [
    "BeetMacol"
  ],
  "contact": {
    "homepage": "https://beetmacol.com/",
    "sources": "https://github.com/BeetMacol/dimensional-teleport"
  },

  "license": "MIT",
  "icon": "assets/dimteleport/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.beetmacol.dimensionalteleport.DimensionalTeleport"
    ]
