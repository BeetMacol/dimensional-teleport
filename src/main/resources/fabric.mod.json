  "mixins": [
    "dimteleport.mixins.json"
  ],
  "accessWidener" : "dimteleport.accesswidener",

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x"
